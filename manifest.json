{
  "manifest_version": 2,
  "name": "Connected Dots",
  "short_name": "Connected Dots",
  "version": "1.0.0",
  "description": "Just connected dots glowing on hover",
  "icons": {
    "16": "src/imgs/packetbot.jpg",
    "48": "src/imgs/packetbot.jpg",
    "128": "src/imgs/packetbot.jpg"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": "src/imgs/packetbot.jpg"
  },
  "permissions": ["storage"]
}
